{
  "name": "camunda-process-statistics-plugin",
  "type": "module",
  "private": true,
  "version": "1.1.1",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "watch": "rollup -w -c --sourcemap",
    "prettier:check": "prettier --check \"src/**/{*.ts,*.tsx}\"",
    "prettier:format": "prettier --write \"src/**/{*.ts,*.tsx}\"",
