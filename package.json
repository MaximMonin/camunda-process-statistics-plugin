    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-icons": "^4.8.0",
    "react-table": "^7.8.0",
    "use-query-params": "^1.2.3"
